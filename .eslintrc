{
  "extends": ["eslint-config-airbnb"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
  ],
  "rules": {
    "constructor-super": 2,
    "eqeqeq": [2, "allow-null"],
    "id-length": 0,
    "one-var": [2, { "initialized": "never" }],
    "no-this-before-super": 2,

    // Google
    "comma-dangle": [2, "never"],
    "arrow-parens": [2, "as-needed"],
    "object-curly-spacing": [2, "never"],
    
    "func-names": 0,
    "strict": 0,
    "generator-star-spacing": 2,
    "quote-props": 0,
    "no-use-before-define": 0,
    "max-len": 0,
    "no-cond-assign": 0,
    "no-console": 0,
    "no-param-reassign": 0
  }
}